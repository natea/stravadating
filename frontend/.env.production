# Production Environment Configuration
# Railway will override these with actual values from environment variables
REACT_APP_API_BASE_URL=https://api.example.com/api
REACT_APP_WEBSOCKET_URL=wss://api.example.com
REACT_APP_STRAVA_CLIENT_ID=your-strava-client-id
REACT_APP_STRAVA_REDIRECT_URI=https://app.example.com/auth/strava/callback
REACT_APP_ENVIRONMENT=production

# Disable ESLint during production builds
DISABLE_ESLINT_PLUGIN=true
ESLINT_NO_DEV_ERRORS=true